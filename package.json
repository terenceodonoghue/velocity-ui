    "react-dom": "^16.9.0",
    "recharts": "^1.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@storybook/react": "^5.1.11",
    "@storybook/theming": "^5.1.11",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
