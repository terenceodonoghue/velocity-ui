{
  "name": "@terenceodonoghue/velocity-ui",
  "version": "1.0.0",
  "description": "React components that implement InVision's Velocity UI kit",
  "main": "lib/index.js",
  "files": [
    "lib",
    "yarn.lock"
  ],
  "repository": "git@github.com:terenceodonoghue/velocity-ui.git",
  "author": "Terence O'Donoghue <1697905+terenceodonoghue@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src/components --ignore 'src/**/*.fixtures.js','src/**/*.spec.jsx','src/**/*.stories.jsx' --out-dir lib --source-maps inline",
    "build-storybook": "build-storybook -c .storybook -o .out -s ./static --quiet",
    "eslint-check": "eslint --print-config src/components/index.js | eslint-config-prettier-check",
    "lint": "eslint --ext .js,.jsx ./ && stylelint './**/*.styles.js'",
    "prebuild": "rimraf lib",
    "prepublish": "yarn run build",
    "storybook": "start-storybook -s ./static --quiet",
    "stylelint-check": "stylelint-config-prettier-check",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/core": "^10.0.21",
    "@emotion/styled": "^10.0.17",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.10.2",
    "recharts": "^1.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@emotion/babel-preset-css-prop": "^10.0.17",
    "@storybook/react": "^5.2.3",
    "@storybook/theming": "^5.2.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "jest-emotion": "^10.0.17",
    "prettier": "^1.18.2",
    "react-scripts": "^3.2.0",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-prettier": "^6.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-prettier": "^1.1.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.1"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "trailingComma": "all"
  }
}
